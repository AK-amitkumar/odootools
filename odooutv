
function sqldump() {
	DATABASE=$1
	DATE=`date +"%F-%H%M"`
	FILE="${DATABASE}_${DATE}.sql"
	echo $FILE
	mysqldump  $DATABASE -uroot -_MYSQL_PASSWORD_ > ~/$FILE
}

alias sync-sams='rsync -var /usr/share/odoo-sams erp.sameskolstyrelsen.se:/usr/share/'
alias sync-kegroup='rsync -var /usr/share/odoo-kegroup skinner.vertel.se:/usr/share/'
alias sql='mysql --column-names=False -u root --password="_MYSQL_PASSWORD_" --batch -e'
